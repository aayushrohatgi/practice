package junit;

import static org.junit.Assert.*;

import java.util.Arrays;

import org.junit.Test;

public class ArrayCompareTest {

	@Test
	public void test() {
		int[] numbers = { 12, 4, 3, 14 };
		int[] expected = { 3, 4, 12, 14 };
		Arrays.sort(numbers);
		assertEquals(expected, numbers);
	}

}
